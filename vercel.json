{
  "functions": {
    "api/test.js": {
      "runtime": "@vercel/node"
    },
    "api/search.js": {
      "runtime": "@vercel/node"
    },
    "api/lookup.js": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/js/(.*)",
      "destination": "/public/js/$1"
    },
    {
      "source": "/css/(.*)",
      "destination": "/public/css/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/public/images/$1"
    },
    {
      "source": "/(.*\\.(jpg|jpeg|png|gif|svg|webp|ico|pdf|txt|md|json))",
      "destination": "/public/$1"
    },
    {
      "source": "/(upload-certificate|about|certification|contact)/?$",
      "destination": "/public/index.html"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/public/index.html"
    }
  ]
}
