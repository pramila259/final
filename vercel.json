{
  "functions": {
    "api/test.js": {
      "runtime": "@vercel/node@18.x"
    },
    "api/search.js": {
      "runtime": "@vercel/node@18.x"
    },
    "api/lookup.js": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "rewrites": [
    {
      "source": "/js/(.*)",
      "destination": "/public/js/$1"
    },
    {
      "source": "/css/(.*)",
      "destination": "/public/css/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/public/images/$1"
    },
    {
      "source": "/logo.png",
      "destination": "/public/logo.png"
    },
    {
      "source": "/gemstone.jpg",
      "destination": "/public/gemstone.jpg"
    },
    {
      "source": "/diamond-sample.jpg",
      "destination": "/public/diamond-sample.jpg"
    },
    {
      "source": "/ruby-sample.jpg",
      "destination": "/public/ruby-sample.jpg"
    },
    {
      "source": "/sapphire-sample.jpg",
      "destination": "/public/sapphire-sample.jpg"
    },
    {
      "source": "/(upload-certificate|about|certification|contact)",
      "destination": "/public/index.html"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/public/index.html"
    }
  ]
}
